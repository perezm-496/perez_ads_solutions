def subset(X, S, m, N, k):
    print(X, S, m, N, k)
    S1 = S.add(X[m])
    if sum(S1) == k:
        return S1
    else if m + 1 <= N:
        x = subset(X, S1, m+1, N, k)
        if not x == None:
            return x
        x = subset(X, S, m+1, N, k)
        if not x == None:
            return x
        return None
    else:
        return None

print(subset([1, 2, 3],set(), 1, 3, 6))
